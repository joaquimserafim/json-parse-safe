{
  "name": "json-parse-safe",
  "version": "1.0.5",
  "description": "Parse your json safely and stop writing try {} catch {}",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover tape test.js",
    "jshint": "jshint -c .jshintrc *.js",
    "code-style": "jscs -p google *.js",
    "check-coverage": "istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "open-coverage": "open coverage/lcov-report/index.html"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joaquimserafim/json-parse-safe.git"
  },
  "keywords": [
    "json",
    "parse",
    "safe",
    "try/catch"
  ],
  "author": "@joaquimserafim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaquimserafim/json-parse-safe/issues"
  },
  "homepage": "https://github.com/joaquimserafim/json-parse-safe",
  "devDependencies": {
    "istanbul": "^0.3.5",
    "jscs": "^1.10.0",
    "jshint": "^2.6.0",
    "pre-commit": "^1.0.2",
    "tape": "^3.5.0"
  },
  "pre-commit": [
    "jshint",
    "code-style",
    "test",
    "check-coverage"
  ]
}
